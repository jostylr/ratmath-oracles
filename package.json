{
  "name": "@ratmath/oracles",
  "version": "0.1.0",
  "description": "This package supports representing real numbers as functions that take in a rational interval and either affirm or deny the presence of the real number in the interval. It uses the library ratmath for the rational interval arithmetic. ",
  "keywords": [
    "real",
    "numbers",
    "rational",
    "arithmetic"
  ],
  "homepage": "https://github.com/jostylr/ratmath-oracles#readme",
  "bugs": {
    "url": "https://github.com/jostylr/ratmath-oracles/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jostylr/ratmath-oracles.git"
  },
  "license": "MIT",
  "author": "jostylr",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "build": "bun tsc",
    "type-check": "bun tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@ratmath/core": "0.2.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^24.10.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}